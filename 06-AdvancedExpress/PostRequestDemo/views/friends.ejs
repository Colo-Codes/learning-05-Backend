<h1>~ ~ ~ Friends List ~ ~ ~</h1>

<% friendsWebpage.forEach(function(friend) { %>
    <li> <%= friend %> </li>
<% }); %>

<form action="/addfriend" method="POST">
    <input type="text" name="newFriend" placeholder="Type friend's name here"> <!-- The "name" is the key. -->
    <button>I made a new friend!</button>
</form>